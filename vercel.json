{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.css",
            "use": "@vercel/public"
        },
        {
            "src": "*.img",
            "use": "@vercel/public"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}